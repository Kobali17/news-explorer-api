{
  "extends": ["airbnb-base"
  ],
  "globals": {
"console": true
  },
  "rules": {
    "no-unused-vars": [2],
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id",
          "_fetch",
          "_headers",
          "_baseUrl"
        ]
      }
    ]
  },

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }

}
